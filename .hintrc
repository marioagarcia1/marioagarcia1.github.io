{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "meta-viewport": "off",
    "disown-opener": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "widows",
          "-webkit-overflow-scrolling"
        ]
      }
    ]
  }
}